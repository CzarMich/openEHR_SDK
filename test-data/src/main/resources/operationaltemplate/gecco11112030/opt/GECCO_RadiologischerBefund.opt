                        <term_definitions code="at0000">
                          <items id="description">Ergebnis der Bildgebung als Einzelwert oder als eine verschachtelte Gruppe von Ergebniswerten.</items>
                          <items id="text">Ergebnis der Bildgebung</items>
                        </term_definitions>
                        <term_definitions code="at0001">
                          <items id="description">Ein spezifisches detailliertes Ergebnis der Bildgebung, das sowohl den Wert des Ergebnisses, als auch zusätzliche Informationen enthält, die für die klinische Interpretation nützlich sein können.</items>
                          <items id="text">Ergebnis der Bildgebung</items>
                        </term_definitions>
                        <term_definitions code="at0002">
                          <items id="description">Der tatsächliche Wert des Ergebnisses der Bildgebung. Der Name dieses Elements kann während der Template Erstellung oder zur Laufzeit, entsprechend der tatsächlichen Bezeichnung, definiert werden. Dieser Wert wird oft kodiert.</items>
                          <items id="text">Ergebnis</items>
                        </term_definitions>
                        <term_definitions code="at0003">
                          <items id="description">Zusätzliche Informationen über das Ergebnis, die nicht im Ergebnisfeld erfasst wurden.</items>
                          <items id="text">Kommentar</items>
                        </term_definitions>
                        <term_definitions code="at0004">
                          <items id="description">Der Status des Ergebniswerts.</items>
                          <items id="text">Status des Ergebnisses</items>
                        </term_definitions>
                        <term_definitions code="at0005">
                          <items id="description">Noch kein Ergebnis verfügbar.</items>
                          <items id="text">Registriert</items>
                        </term_definitions>
                        <term_definitions code="at0006">
                          <items id="description">Dies ist ein erstes oder ein vorläufiges Ergebnis: es können Daten fehlen oder die Prüfung des Ergebnisses wurde noch nicht durchgeführt.</items>
                          <items id="text">Vorläufig</items>
                        </term_definitions>
                        <term_definitions code="at0007">
                          <items id="description">Das Ergebnis ist komplett und wurde vom zuständigen Pathologen überprüft.</items>
                          <items id="text">Final</items>
                        </term_definitions>
                        <term_definitions code="at0008">
                          <items id="description">Das Ergebnis wurde nach der Finalisierung modifiziert und ist nun vollständig und vom zuständigen Pathologen verifiziert.</items>
                          <items id="text">Geändert</items>
                        </term_definitions>
                        <term_definitions code="at0009">
                          <items id="description">Das Ergebnis ist nicht verfügbar, da der Test nicht begonnen oder nicht abgeschlossen wurde.</items>
                          <items id="text">Storniert/Abgebrochen</items>
                        </term_definitions>
                        <term_definitions code="at0010">
                          <items id="description">Das Ergebnis ist nicht verfügbar, weil der Test nicht angefordert wurde.</items>
                          <items id="text">Nicht angefordert</items>
                        </term_definitions>
                        <term_definitions code="at0011">
                          <items id="description">Das Datum und/oder die Uhrzeit des ausgegebenen Status des Ergebnisses.</items>
                          <items id="text">Zeitstempel vom Status des Ergebnisses</items>
                        </term_definitions>
                        <term_definitions code="at0012">
                          <items id="description">Weitere Details zu einem einzelnen Ergebnis.</items>
                          <items id="text">Details zum Ergebnis der Bildgebung</items>
                        </term_definitions>
                        <term_definitions code="at0013">
                          <items id="description">Details über die individuelle anatomische Position, auf die sich das Ergebnis bezieht, wenn eine präzisere Wiedergabe der anatomischen Position erforderlich ist.</items>
                          <items id="text">Anatomische Lageposition</items>
                        </term_definitions>
                        <term_definitions code="at0014">
                          <items id="description">Weitere Details zu einem einzelnen Ergebnis oder einem gruppiertem Ergebnis oder zu einer verschachtelten Gruppe von Ergebniswerten.</items>
                          <items id="text">Andere Details</items>
                        </term_definitions>
